{
  "hosting": {
    "public": "astro-blog/dist",
    "trailingSlash": false,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.@(js)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000000, immutable"
          }
        ]
      },
      {
        "source": "**/*.@(css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000000, immutable"
          }
        ]
      },
      {
        "source": "**/*.@(png)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000000, immutable"
          }
        ]
      },
      {
        "source": "**/*.@(jpg)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000000, immutable"
          }
        ]
      },
      {
        "source": "**/*.@(otf)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000000, immutable"
          }
        ]
      }
    ]
  }
}